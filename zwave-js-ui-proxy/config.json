{
    "name": "Z-Wave JS UI Proxy",
    "version": "0.1.6",
    "slug": "zwave_js_ui_proxy",
    "description": "Proxy for externally running Z-wave JS UI",
    "url": "https://github.com/Calimerorulez/hassio-addons/tree/master/zwave-js-ui-proxy",
    "arch": [
      "aarch64",
      "amd64"
    ],
    "boot": "auto",
    "init": true,
    "true": true,
    "ingress": true,
    "timeout": 30,
    "panel_icon": "mdi:zwave",
    "map": [
      "config:ro"
    ],
    "ports": {
      "8091/tcp": null
    },
    "ports_description": {
      "8091/tcp": "Frontend tcp-listen port"
    },
    "options": {
      "server": null
    },
    "schema": {
      "server": "url"
    }
}
