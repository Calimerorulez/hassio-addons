{
  "name": "Node-Red Proxy",
  "version": "1.0.2",
  "url": "https://github.com/Calimerorulez/hassio-addons/nodered-proxy",
  "panel_icon": "mdi:transit-connection-variant",
  "slug": "nodered-proxy",
  "description": "Proxy for externally hosted Node-Red",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "startup": "application",
  "boot": "auto",
  "ingress": true,
  "init": false,
  "ingress_port": 1880,
  "panel_admin": false,
  "options": {
    "server": "node-red.local:1880"
  },
  "schema": {
    "server": "match(^.+:\\d+$)"
  }
}
