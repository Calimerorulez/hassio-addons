{
    "name": "ESPHome Proxy",
    "version": "0.1.3",
    "slug": "esphome_proxy",
    "description": "Proxy for externally running ESPHome",
    "url": "https://github.com/Calimerorulez/hassio-addons/tree/master/esphome-proxy",
    "arch": [
      "aarch64",
      "amd64",
      "armhf",
      "armv7",
      "i386"
    ],
    "boot": "auto",
    "init": true,
    "true": true,
    "ingress": true,
    "timeout": 30,
    "panel_icon": "mdi:chip",
    "map": [
      "config:ro"
    ],
    "ports": {
      "6052/tcp": null
    },
    "ports_description": {
      "6052/tcp": "Frontend tcp-listen port"
    },
    "options": {
      "server": null,
      "auth_token": ""
    },
    "schema": {
      "server": "url",
      "auth_token": "str?"
    }
}
